{
    "ecmaFeatures": {
		"impliedStrict": true
    },
    "rules": {
        "eqeqeq": 1,
        "indent": [2, "tab", {"SwitchCase": 1}],
        "no-unused-vars": 1,
        "curly": 1,
        "no-multi-spaces": 2,
        "no-shadow": 2,
        "comma-dangle": [2, "never"],
        "no-use-before-define": 0,
        "no-empty": 1,
        "no-cond-assign": 2,
        "no-undef": 2,
		"no-extra-semi": 2,
		"semi": [2, "always"]
    },
    "env": {
        "node": true,
        "browser": true,
        "jquery": true
    },
	"globals": {
		"CdfConfig": true,
		"CdfBlock": true,
		"CdfTemplate": true
	}
}
